<h1> All the Wiki you want!</h1>
<div class="col-md-8">
    <% @wikis.each do |wiki| %>
        <div class="media">
            <div class="media-body">
                <h4 class="media-heading">
                    <%= link_to wiki.title, wiki %> 
                    <% if wiki.private %>
                     - Private
                    <% end %>
                </h4>
            </div>
        </div>
    <% end %>
</div>

<div class="col-md-4 pull-right">
    <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
</div>